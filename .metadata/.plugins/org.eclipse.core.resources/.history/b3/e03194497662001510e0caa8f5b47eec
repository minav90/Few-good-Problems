#include <iostream>
#include <vector>
using namespace std;

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	vector<int> arr(2);
	arr = { 34,23 };
	int curr = arr.front();
	cout << curr << endl;



	return 0;
}
