#include <iostream>
#include <vector>
using namespace std;

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	vector<int> arr = {1,2,3,4};
	int curr = arr.front();
	while(curr != arr.end())
	{
		cout<<curr<<endl;
		curr++;
	}



	return 0;
}
