import java.util.ArrayList;

public class Node {
	
	

	public Node(ArrayList<ArrayList<String>> state) {
		this.state = state;
	}

	ArrayList<ArrayList<String>> state = new ArrayList<ArrayList<String>>();

	public ArrayList<ArrayList<String>> getState() {
		return state;
	}

	public void setState(ArrayList<ArrayList<String>> state) {
		this.state = state;
	}
	
	
	
	
	
}


