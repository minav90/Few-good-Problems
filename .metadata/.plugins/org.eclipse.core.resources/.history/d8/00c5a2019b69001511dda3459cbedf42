
public class AdvancedEnemyBot extends EnemyBot {
	
	private double x;
	private double y;
	public double getX() {
		return x;
	}
	public void setX(double x) {
		this.x = x;
	}
	public double getY() {
		return y;
	}
	public void setY(double y) {
		this.y = y;
	}
	
	@Override
	public void reset() {
		// TODO Auto-generated method stub
		super.reset();
		setX(0.0);
		setY(0.0);
	}

	
}
