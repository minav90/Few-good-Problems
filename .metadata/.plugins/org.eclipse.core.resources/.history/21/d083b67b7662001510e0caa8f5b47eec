#include <iostream>
#include <vector>
using namespace std;

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	static const int arr[] = {16,2,77,29};
	vector<int> vec (arr, arr + sizeof(arr) / sizeof(arr[0]) );
	int curr = vec.front();
	cout << curr << endl;



	return 0;
}
