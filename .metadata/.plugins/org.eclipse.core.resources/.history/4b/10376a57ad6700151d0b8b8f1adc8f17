import java.util.Random;

public class Tour {

	public double getDistance()
	{ 
		City source = TourManager.getCity(getRanInt(1,TourManager.getTourSize()));
		City dest = TourManager.getCity(getRanInt(1,TourManager.getTourSize()));
		
		
       
		
		
		return 0;
		
	}
	
	public int getRanInt(int min,int max)
	{
		Random random = new Random();
		return (random.nextInt(max - min + 1) + min);
	}
	
	
}
